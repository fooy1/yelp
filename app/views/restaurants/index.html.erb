<h1>Restaurants</h1>

<%= link_to 'Add New Restaurant', new_restaurant_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restaurants.each do |restaurant| %>
      <tr>
        <td><%= restaurant.name %></td>
        <td><%= restaurant.category %></td>
        <td><%= link_to 'Show', restaurant %></td>
      </tr>
    <% end %>
  </tbody>
</table>
